#:kivy 1.10.1

<Label>:
    color: 0,0,0,1
    

<PoytakirjaScreen>:
    name: "Poytakirja"
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'wallpaper.png' if root.jokupaska else "valkone.jpg"

    on_enter:
        root.get_jengit()
        root.get_players_koti(self.kotinippu1)
        root.get_players_vieras(self.vierasnippu1)
        root.nimea_jengit(self.kotinippu1, self.vierasnippu1)
    
    BoxLayout:
        orientation: 'vertical'
        spacing: 10

        GridLayout:
            id: ylaInputit
            padding: [100,0,200,0]
            size_hint: 1, None
            size_hint_y: None
            height: 50
            cols: 6
            Label:
                text: "Sarja:" if app.kielisettings else "League"
                size_hint_x: 0.1
            TextInput:
                id: sarja
                size_hint: (.35, None)
                height: 30
                multiline: False
                write_tab: False
            Label:
                text: "Ott.nr:" if app.kielisettings else "Game no:"
                size_hint_x: 0.1
            TextInput:
                id: ottnro
                size_hint: (.1, None)
                height: 30
                multiline: False
                write_tab: False
            Label:
                text: "Alkoi:" if app.kielisettings else "Started:"
                size_hint_x: 0.1
            TextInput:
                id: alkuaika
                size_hint: (.1, None)
                height: 30
                multiline: False
                write_tab: False
            Label:
                text: "Paikka ja pvm" if app.kielisettings else "Location and date"
                size_hint_x: 0.1
            TextInput:
                id: paikkapvm
                size_hint: (.30, None)
                height: 30
                multiline: False
                write_tab: False
            Label:
                text: "Yleiso:" if app.kielisettings else "Crowd"
                size_hint_x: 0.1
            TextInput:
                id: yleiso
                size_hint: (.1, None)
                height: 30
                multiline: False
                write_tab: False
            Label:
                text: "Paattyi:" if app.kielisettings else "Ended"
                size_hint_x: 0.1
            TextInput:
                id: paattymisaika
                size_hint: (.1, None)
                height: 30
                multiline: False
                write_tab: False
     
        GridLayout:
            size_hint: 1, None
            size_hint_y: 0.1
            height: 50
            cols: 5
            padding: [0,0,500,0]
            Label:
                text: "Koti"
